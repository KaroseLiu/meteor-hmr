{
  "presets": [ "meteor" ],
  "plugins": [
    "transform-decorators-legacy",
    /*[
      "transform-import-rewrite",
      {
        "a": 1
      }
    ]*/
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }, {
            "transform": "react-transform-catch-errors",
            "imports": ["react", "redbox-react"]
          }]
        }]
      ]
    }
  }
}
